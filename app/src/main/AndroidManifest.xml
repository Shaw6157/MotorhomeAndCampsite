<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.ais.mnc">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".view.activity.SplashActivity">
        </activity>
        <activity android:name=".view.activity.UserLoginActivity"></activity>
        <activity android:name=".view.activity.UserSignUpActivity"></activity>
        <activity
            android:name=".view.activity.CampsiteListActivity"
            android:label="@string/clist_title"
            android:theme="@style/AppTheme" />
        <activity android:name=".view.activity.CampsiteDetailActivity"></activity>
        <activity android:name=".view.activity.CampsiteMapActivity"></activity>
        <activity android:name=".view.activity.PhotoListActivity"></activity>
        <activity android:name=".view.activity.PhotoDetailActivity"></activity>

        <activity android:name=".view.activity.VehicleListActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".view.activity.VehicleDetailActivity"></activity>
        <activity android:name=".view.activity.OrderListActivity"></activity>
        <activity android:name=".view.activity.OrderDetailActivity"></activity>
    </application>

</manifest>